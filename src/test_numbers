SELECT * FROM contacts WHERE id > 2;
SELECT * FROM contacts WHERE id <= 2;
SELECT * FROM contacts WHERE id < 2;
SELECT * FROM contacts WHERE id >= 2;

SELECT * FROM contacts WHERE name LIKE "%Rem";
SELECT * FROM contacts WHERE name LIKE "%m%";
SELECT * FROM contacts WHERE name LIKE "m%";

